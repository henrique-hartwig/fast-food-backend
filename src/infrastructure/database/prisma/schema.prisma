generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Order {
  id        String   @id @default(cuid())
  items     String[]
  total     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
